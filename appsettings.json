{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information"
    }
  },
  "AllowedHosts": "*",
  "OpenIdConnect": {
    "Name": "OpenIdConnect",
    "CallBackPath": "/oauth-callback",
    "IdPSignOutPath": "<idp sign out path>",
    "PostSignOutRedirect": "<fe post sign out url>",
    "Authority": "<idp url>",
    "ClientId": "<client id>",
    "ClientSecret": "<client secret>",
    "Scopes": "openid offline_access",
    "UsePkce": true,
    "AcrValues": "<acr values>",
    "UiLocales": "<ui locales>"
  },
  "AllowedOrigins": ["<fe url>"]
}
